alias sufp="sudo featherpad"
alias sufm="pkexec env DISPLAY=$DISPLAY XAUTHORITY=$XAUTHORITY KDE_SESSION_VERSION=5 KDE_FULL_SESSION=true dolphin"
alias update-grub="grub-mkconfig -o /boot/grub/grub.cfg"
alias aptfromtxt="sudo xargs -P1 apt-get install <"
alias squeakycleanremove="echo WARNING USE AT YOUR OWN RISK && sudo apt purge --autoremove"
alias fullupdate="sudo apt update && sudo apt upgrade && sudo apt full-upgrade"
alias clean="sudo apt clean && sudo apt update && sudo apt autoremove --purge"
alias dumppkg="sudo dpkg-query -f '${binary:Package}\n' -W > packages_list.txt"
alias resetswap="sudo swapoff -a && sudo swapon -a"
